<div class="outer">
	<div style="margin: 0.5em 0">Rooms:</div>
		<%= for {room_name, num_users} <- @rooms do %>
		<div class="room">
			<div style="display: inline-block">
				<a class="btn_room" href="/r/<%= room_name %>">
					<div>
						<span><%= room_name %></span><span style="float: right;"><%= num_users %> 🙍</span>
					</div>
				</a>
			</div>
		</div>
	<% end %>
</div>

<link rel="stylesheet" type="text/css" href="/css/index.css">
<script src="/js/index.js"></script>
